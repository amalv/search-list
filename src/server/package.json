{
  "name": "search-list",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.11.7",
    "body-parser": "^1.20.1",
    "concurrently": "^7.5.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "build:server": "npx tsc --watch ",
